<h2 class="center">Subscriber Data for <%= course.name %></h2>
<table class ="table">
    <tbody>
            <tr> <!--this makes a row-->
                <td>Title:</td>
                <td>
                    <%= course.title %>
                </td>
            </tr>
            
            <tr>
                <td>Description:</td>
                <td>
                    <%= course.description %>
                </td>
            </tr>
            <tr>
                <td>Max Students: </td>
                <td>
                    <%= course.maxStudents %>
                </td>
            </tr>
            <tr>
                <td>Course Cost: </td>
                <td> <%= course.cost %> </td>
            </tr>
    </tbody>
</table>

<a href="/courses"> 
    <span class="button">View all Courses</span>
</a>